# -11-603

// ПЕРЕВОРАЧИВАЕМ МАССИВ

public class Lesson1{

	public static void main(String[] args){

		int a[] = {1,2,3,4,5,6,7,8};
		int b = a.length-1;

		for(int i=0;i < (a.length/2) ;i++){

			int save = a[i];
			a[i] = a[b];
			a[b] = save;
			b--;
		}

		for(int i=0;i<a.length;i++){

			System.out.println(a[i]);

		}
	}
}
